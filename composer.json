{
    "name": "dayonevn/gotit-merchant-php-sdk",
    "description": "Technical document APIs for Merchant APIs",
    "keywords": [
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "duong.vu"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "Dayonevn\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Dayonevn\\Test\\" : "test/" }
    }
}
